{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Debug",
			"type": "shell",
			"command": ".vscode/buildDebug.sh",
			"problemMatcher": [],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Build Release",
			"type": "shell",
			"command": ".vscode/buildRelease.sh",
			"problemMatcher": [],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build"
		},
		{
			"label": "Build All",
			"type": "shell",
			"command": ".vscode/buildAll.sh",
			"problemMatcher": [],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build"
		},
		{
			"label": "Fix",
			"type": "shell",
			"command": ".vscode/fix.sh",
			"problemMatcher": [],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none"
		}
	]
}