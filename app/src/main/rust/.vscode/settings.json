{
    "workbench.colorCustomizations": {
        "activityBar.background": "#3E2347",
        "titleBar.activeBackground": "#573163",
        "titleBar.activeForeground": "#FBF9FC"
    },
    "rust-analyzer.checkOnSave.overrideCommand": [
        "cargo",
        "ndk",
        "-p",
        "30",
        "-t",
        "x86_64",
        "check",
        "--message-format=json",
    ],
    "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "cargo",
        "ndk",
        "-p",
        "30",
        "-t",
        "x86_64",
        "check",
        "--message-format=json",
    ],
    "rust-analyzer.trace.server": "verbose",
    "rust-analyzer.server.extraEnv": {
        "PROJECT_ROOT": "${env:HOME}/AndroidStudioProjects/WhisperVoiceKeyboard",
        "JNI_LIB_DIR": "${env:PROJECT_ROOT}/app/src/main/jniLibs",
        "TFLITEC_PREBUILT_PATH_X86_64_LINUX_ANDROID": "${env:JNI_LIB_DIR}/x86_64/libtensorflowlite_c.so",
        "TFLITEC_PREBUILT_PATH_AARCH64_LINUX_ANDROID": "${env:JNI_LIB_DIR}/arm64-v8a/libtensorflowlite_c.so",
        "ANDROID_NDK_API_LEVEL": 30,
        "ANDROID_SDK_VERSION": 30,
        "ANDROID_API_LEVEL": 30,
        "ANDROID_BUILD_TOOLS_VERSION": "33.0.0",
        "ANDROID_SDK_HOME": "${env:HOME}/Android/Sdk",
        "ANDROID_HOME": "${env:ANDROID_SDK_HOME}/build-tools/${env:ANDROID_BUILD_TOOLS_VERSION}",
        "ANDROID_NDK_HOME": "${env:ANDROID_SDK_HOME}/ndk/25.1.8937393",
        "BINDGEN_EXTRA_CLANG_ARGS_X86_64_LINUX_ANDROID": "-I${env:ANDROID_NDK_HOME}/sources/cxx-stl/llvm-libc++/include/ -I${env:ANDROID_NDK_HOME}/sysroot/usr/include/ -I${env:ANDROID_NDK_HOME}/toolchains/llvm/prebuilt/linux-x86_64/sysroot/usr/include/x86_64_linux_android/",
        "BINDGEN_EXTRA_CLANG_ARGS_AARCH64_LINUX_ANDROID": "-I${env:ANDROID_NDK_HOME}/sources/cxx-stl/llvm-libc++/include/ -I${env:ANDROID_NDK_HOME}/sysroot/usr/include/ -I${env:ANDROID_NDK_HOME}/toolchains/llvm/prebuilt/linux-x86_64/sysroot/usr/include/aarch64_linux_android/"
    },
    "rust-analyzer.cargo.buildScripts.invocationLocation": "workspace",
}